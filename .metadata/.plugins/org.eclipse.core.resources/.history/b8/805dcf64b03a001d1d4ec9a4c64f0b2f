import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		String x;
		int b;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite seu nome: ");
		
		x = sc.next();
		
		System.out.println("Voce digitou: " + x);
		
		System.out.println("Digite um numero: ");
		
		sc.close();
		
		
	}

}
